{
  "name": "frenchie-trivia-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create frenchie-trivia-db",
    "db:migrate": "wrangler d1 execute frenchie-trivia-db --file=./migrations/001_initial_schema.sql",
    "db:seed": "wrangler d1 execute frenchie-trivia-db --file=./seeds/questions.sql"
  },
  "dependencies": {
    "hono": "^4.6.0",
    "@tsndr/cloudflare-worker-jwt": "^3.0.0",
    "@upstash/redis": "^1.34.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250101.0",
    "typescript": "^5.3.0",
    "wrangler": "^4.0.0"
  }
}
